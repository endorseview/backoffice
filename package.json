{
	"name": "fuse",
	"version": "8.0.0",
	"license": "https://themeforest.net/licenses/terms/regular",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --open",
		"startssl": "ng serve --port=4200 --ssl --ssl-key ~/localcert/server.key  --ssl-cert ~/localcert/server.crt",
		"start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
		"start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
		"build": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng build",
		"build-stats": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng build --stats-json",
		"build-prod": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod",
		"build-prod-stats": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"bundle-report": "webpack-bundle-analyzer dist/stats.json",
		"extract": "ngx-translate-extract --input ./src/app/ --output ./src/assets/i18nExt/{en,tr}.json --clean --sort --format namespaced-json"
	},
	"private": true,
